{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "sweetalert2@11.14.0",
      "destination": "wwwroot/lib/sweetalert2/",
      "files": [
        "sweetalert2.min.js",
        "sweetalert2.min.css"
      ]
    },
    {
      "library": "qrcodejs@1.0.0",
      "destination": "wwwroot/lib/qrcodejs/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@8.0.7",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}